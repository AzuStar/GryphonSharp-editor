{
	"name": "gryphonsharp-vscode",
	"displayName": "G# vscode",
	"description": "Gryphon# visual editor",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:gsharp.nodeEditor"
	],
	"main": "./out/extension/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "gsharp.nodeEditor",
				"displayName": "G# Node Editor",
				"selector": [
					{
						"filenamePattern": "*.gs"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./src/editor && sc -watch -p ./src/extension",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./src/editor && sc -watch -p ./src/extension",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.8",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.45",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"konva": "^7.2.5",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"tsd": "^0.14.0"
	}
}
